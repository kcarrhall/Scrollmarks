{
  "manifest_version": 2,

  "name": "Scrollmarks",
  "description": "Saves your scroll location & scrolls to it on page reload",
  "version": "0.1.3",
  "background": {
      "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/transparentIcon.png",
    "default_title": ""
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}